void main() {

 String day = "Tuesday";

  switch(day){
    case 'Monday':
      print("Hey, it's Monday");
      break;
      case 'Tuesday':
      print("Hey, it's Tuesday");
      break;
       case 'Wednesday':
      print("Hey, it's Wednesday");
      break;
       case 'Thursday':
      print("Hey, it's Thursday");
      break;
       case 'Friday':
      print("Hey, it's Friday");
      break;
       case 'Saturday':
      print("Hey, it's Saturday");
      break;
      case 'Sunday':
      print("Hey, it's Sunday");
      break;
      
  }
}
